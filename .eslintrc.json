{
  "extends": ["./node_modules/aurelia-tools/.eslintrc.json", "plugin:react/recommended", "airbnb",
    "plugin:jest/recommended", "plugin:security/recommended", "plugin:node/recommended"
  ],
  "rules": {
    "security/detect-possible-timing-attacks": 0,
    "node/no-extraneous-require": 0,
    "react/button-has-type": 0,
    "no-shadow": 0,
    "prefer-rest-params": 0,
    "global-require": 0,
    "no-use-before-define": 0,
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "no-unused-vars": 0,
    "security/detect-object-injection": 0,
    "node/no-unsupported-features/es-syntax": 0,
    "jest/no-identical-title": 0,
    "no-mixed-operators": 0,
    "node/no-unpublished-require": 0,
    "no-useless-escape": 0,
    "comma-dangle": [0, "never"],
    "no-undef": [0, "never"],
    "react/jsx-filename-extension": [0, "never"],
    "no-underscore-dangle": [0, "never"],
    "no-alert": [0, "never"],
    "func-names": [0, "never"],
    "class-methods-use-this": [0, "never"],
    "no-restricted-syntax": [0, "never"],
    "prefer-destructuring": 0,
    "no-param-reassign": [0, "never"],
    "import/prefer-default-export": [0, "never"],
    "max-len": ["error", {
      "code": 150
    }],
    "one-var-declaration-per-line": [0, "never"],
    "lines-between-class-members": [0, "never"],
    "one-var": [2, {
      "var": "never",
      "let": "always",
      "const": "never"
    }]
  }
}
