{
  "name": "web-jam-front",
  "version": "0.1.0",
  "description": "Front End of web-jam.com App",
  "main": "dist/app.bundle.js",
  "repository": {
    "url": "git+ssh://git@github.com/WebJamApps/combined-front.git",
    "type": "git"
  },
  "author": "HandlersAndKickers",
  "license": "MIT",
  "engines": {
    "node": "7.10.0",
    "npm": "4.2.0"
  },
  "scripts": {
    "start": "nps",
    "test": "npm run test:karma && nps e2e",
    "test:karma": "cross-env NODE_ENV=test nps test.lint && nps test.karma",
    "test:debug": "cross-env NODE_ENV=test nps test.karma.debug",
    "cleaninstall": "rm -rf bower_components && rm -rf yarn.lock && rm -rf node_modules && npm i -g yarn && yarn cache clean && yarn install && npm run postinstall:backend",
    "postinstall:backend": "yarn global add bower && bower install && npm run vulc && npm start -- build",
    "vulc": "rm -rf static/includes.html && polymer-bundler --inline-scripts --inline-css includes.html > static/includes.html",
    "prod": "npm run vulc && npm start -- build && npm start -- serve",
    "snyk-protect": "snyk protect",
    "eslint-fix": "./node_modules/eslint/bin/eslint.js --fix .",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "au-table": "^0.1.12",
    "aurelia-api": "^3.1.1",
    "aurelia-auth": "^3.0.5",
    "aurelia-bootstrapper": "2.1.1",
    "aurelia-config": "^0.1.1",
    "aurelia-fetch-client": "1.1.2",
    "aurelia-framework": "1.1.2",
    "aurelia-history-browser": "1.0.0",
    "aurelia-loader-nodejs": "1.0.1",
    "aurelia-loader-webpack": "2.1.0",
    "aurelia-logging-console": "1.0.0",
    "aurelia-pal-browser": "1.2.1",
    "aurelia-pal-nodejs": "1.0.0-beta.1.0.0",
    "aurelia-polyfills": "1.2.1",
    "aurelia-polymer": "^1.0.0",
    "aurelia-router": "1.3.0",
    "aurelia-templating": "1.4.2",
    "aurelia-templating-binding": "1.3.0",
    "aurelia-templating-resources": "1.4.0",
    "aurelia-templating-router": "1.1.0",
    "aurelia-validation": "^1.0.0",
    "aurelia-webpack-plugin": "2.0.0-rc.2",
    "autoprefixer": "7.0.0",
    "babel-core": "next",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "next",
    "babel-plugin-transform-decorators": "next",
    "babel-polyfill": "next",
    "babel-preset-env": "next",
    "babel-register": "next",
    "bluebird": "3.5.0",
    "bootstrap": "^3.3.7",
    "bower": "^1.8.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "4.0.0",
    "css-loader": "^0.28.1",
    "csvjson": "^4.1.3",
    "dotenv": "^4.0.0",
    "expose-loader": "0.7.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "file-saver": "^1.3.3",
    "font-awesome": "4.7.0",
    "global": "^4.3.2",
    "html-webpack-plugin": "2.28.0",
    "isomorphic-fetch": "2.2.1",
    "jest": "19.0.2",
    "jquery": "3.2.1",
    "json-loader": "^0.5.4",
    "nps": "^5.1.0",
    "nps-utils": "^1.2.0",
    "postcss-loader": "^1.3.3",
    "pushstate-server": "^3.0.0",
    "snyk": "^1.34.2",
    "style-loader": "^0.17.0",
    "url-loader": "0.5.8",
    "polymer-bundler": "^2.0.1",
    "wait-on": "2.0.2",
    "webpack": "^2.5.1"
  },
  "devDependencies": {
    "assertions-counter": "^0.0.1",
    "aurelia-protractor-plugin": "1.0.3",
    "aurelia-testing": "1.0.0-beta.3.0.1",
    "aurelia-tools": "1.0.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-istanbul": "4.1.3",
    "eslint": "^3.19.0",
    "html-hint": "^0.2.4",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.6.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "1.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.3",
    "protractor": "5.1.1",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "webpack-dev-server": "2.4.5"
  },
  "snyk": true
}
