{
  "name": "combined-front",
  "version": "0.7.5",
  "description": "Front End of web-jam.com App",
  "main": "dist/index.html",
  "repository": {
    "url": "git+ssh://git@github.com/WebJamApps/combined-front.git",
    "type": "git"
  },
  "author": "HandlersAndKickers",
  "license": "MIT",
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "scripts": {
    "start": "nps",
    "test": "rm -rf coverage && npm run test:jest && npm run test:karma && nps e2e && npm run combine-coverage",
    "combine-coverage": "istanbul-combine -d coverage -p none -r lcov -r html coverage/jest/coverage-final.json coverage/karma/coverage-final.json",
    "test:jest": "cross-env NODE_ENV=test jest --config jest.config.json --notify",
    "test:karma": "cross-env NODE_ENV=test nps test.lint && nps test.karma",
    "test:debug": "cross-env NODE_ENV=test nps test.karma.debug",
    "installglobals": "npm i -g npm@latest && npm i -g bower@latest",
    "cleaninstall": "rm -rf bower_components && rm -rf yarn.lock && rm -rf node_modules && yarn cache clean && npm run installglobals && yarn install && npm run postinstall:backend",
    "postinstall:backend": "bower install && npm run vulc && npm start -- build",
    "vulc": "rm -rf static/includes.html && polymer-bundler --inline-scripts --inline-css includes.html > static/includes.html",
    "prod": "npm run vulc && npm start -- build && npm start -- serve",
    "snyk-protect": "snyk protect",
    "eslint:fix": "./node_modules/eslint/bin/eslint.js --fix .",
    "fix:aurelia-val": "rm -rf ./node_modules/aurelia-validation/node_modules",
    "postinstall": "npm run fix:aurelia-val",
    "prepublish": "npm run snyk-protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "jsdom": "11.5.1",
    "au-table": "^0.1.12",
    "aurelia-api": "^3.1.1",
    "aurelia-auth": "^3.0.5",
    "aurelia-bootstrapper": "^2.1.1",
    "aurelia-config": "^0.1.1",
    "aurelia-fetch-client": "^1.1.2",
    "aurelia-framework": "^1.1.4",
    "aurelia-history-browser": "^1.1.0",
    "aurelia-loader-nodejs": "^1.0.1",
    "aurelia-loader-webpack": "^2.1.0",
    "aurelia-logging-console": "^1.0.0",
    "aurelia-pal-browser": "^1.2.1",
    "aurelia-pal-nodejs": "^1.0.0-beta.3.0.0",
    "aurelia-polyfills": "^1.2.2",
    "aurelia-polymer": "^1.0.0",
    "aurelia-router": "^1.4.0",
    "aurelia-templating": "^1.5.0",
    "aurelia-templating-binding": "^1.4.0",
    "aurelia-templating-resources": "^1.5.0",
    "aurelia-templating-router": "^1.2.0",
    "aurelia-validation": "^1.1.2",
    "aurelia-webpack-plugin": "^2.0.0-rc.5",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.0",
    "bootstrap": "^4.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "csvjson": "^4.1.3",
    "dotenv": "^4.0.0",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "file-saver": "^1.3.3",
    "font-awesome": "4.7.0",
    "global": "^4.3.2",
    "html-webpack-plugin": "^2.29.0",
    "inputmask": "^3.3.7",
    "isomorphic-fetch": "2.2.1",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.4",
    "minimatch": "^3.0.2",
    "nps": "^5.6.0",
    "nps-utils": "^1.2.0",
    "polymer-bundler": "^2.0.1",
    "popper.js": "^1.12.9",
    "postcss": "^6.0.14",
    "postcss-loader": "^2.0.6",
    "snyk": "^1.69.3",
    "style-loader": "^0.18.2",
    "url-loader": "^0.6.2",
    "wait-on": "^2.0.2",
    "webpack": "^3.7.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "assertions-counter": "^0.0.1",
    "aurelia-protractor-plugin": "^1.0.3",
    "aurelia-testing": "^1.0.0-beta.3.0.1",
    "aurelia-tools": "^1.0.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-istanbul": "^4.1.4",
    "eslint": "^3.19.0",
    "html-hint": "^0.2.4",
    "istanbul": "^0.4.5",
    "istanbul-combine": "^0.3.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.6.4",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jest-environment-jsdom-11.0.0": "^20.0.9",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "postcss-sass": "^0.2.0",
    "protractor": "^5.1.2",
    "pushstate-server": "^3.0.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^17.0.0",
    "webpack-dev-server": "^2.6.1"
  },
  "snyk": true
}
