{
  "name": "combined-front",
  "version": "0.7.9",
  "description": "Front End of web-jam.com App",
  "main": "dist/index.html",
  "repository": {
    "url": "git+ssh://git@github.com/WebJamApps/combined-front.git",
    "type": "git"
  },
  "author": "HandlersAndKickers",
  "license": "MIT",
  "engines": {
    "node": "8.11.3"
  },
  "scripts": {
    "start": "nps",
    "test": "rm -rf coverage && npm run test:jest && npm run test:jest-react && npm run test:karma && npm run combine-coverage && npm run webdriver-update && nps e2e && npm run test:lint",
    "combine-coverage": "istanbul-combine -d coverage -p none -r lcov -r html coverage/jest-react/coverage-final.json coverage/jest/coverage-final.json coverage/karma/coverage-final.json",
    "test:jest": "cross-env NODE_ENV=test && jest --no-cache --config jest.config.json --notify",
    "protest": "nps test",
    "test:jest-react": "nps test.react",
    "test:jest-react-u": "nps test.react.accept",
    "test:karma": "cross-env NODE_ENV=test nps test.karma",
    "test:debug": "cross-env NODE_ENV=test nps test.karma.debug",
    "test:lint": "nps test.lint",
    "installglobals": "npm i -g npm@latest",
    "cleaninstall": "rm -rf yarn.lock && rm -rf node_modules && yarn cache clean && npm run installglobals && yarn install",
    "snyk-protect": "snyk protect",
    "eslint:fix": "./node_modules/eslint/bin/eslint.js --fix .",
    "fix:aurelia-val": "rm -rf ./node_modules/aurelia-validation/node_modules",
    "postinstall": "npm run fix:aurelia-val && npm start -- build",
    "webdriver-update": "webdriver-manager update"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.1",
    "au-table": "^0.1.12",
    "aurelia-api": "^3.1.1",
    "aurelia-auth": "^3.0.5",
    "aurelia-bootstrapper": "^2.1.1",
    "aurelia-config": "^0.1.1",
    "aurelia-fetch-client": "^1.1.2",
    "aurelia-framework": "^1.1.4",
    "aurelia-history": "^1.1.0",
    "aurelia-history-browser": "^1.1.0",
    "aurelia-loader-nodejs": "^1.0.1",
    "aurelia-loader-webpack": "^2.1.0",
    "aurelia-logging-console": "^1.0.0",
    "aurelia-pal": "^1.8.0",
    "aurelia-pal-browser": "^1.8.0",
    "aurelia-pal-nodejs": "^1.1.1",
    "aurelia-polyfills": "^1.2.2",
    "aurelia-router": "^1.5.0",
    "aurelia-templating": "^1.5.0",
    "aurelia-templating-binding": "^1.4.0",
    "aurelia-templating-resources": "^1.5.0",
    "aurelia-templating-router": "1.2.0",
    "aurelia-validation": "^1.1.2",
    "aurelia-webpack-plugin": "^3.0.0-rc.1",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.0",
    "bootstrap": "^4.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "csvjson": "^4.1.3",
    "dotenv": "^4.0.0",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "file-saver": "^1.3.8",
    "font-awesome": "4.7.0",
    "global": "^4.3.2",
    "hammerjs": "^2.0.8",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "2.2.1",
    "jest-matchers": "^20.0.3",
    "jquery": "^3.2.1",
    "jsdom": "11.5.1",
    "json-loader": "^0.5.4",
    "material-ui-time-picker": "^1.0.0",
    "mini-css-extract-plugin": "^0.4.0",
    "minimatch": "^3.0.2",
    "nps": "^5.6.0",
    "nps-utils": "^1.2.0",
    "popper.js": "^1.12.9",
    "postcss": "^6.0.14",
    "postcss-loader": "^2.0.6",
    "postcss-sass": "^0.2.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-player": "^1.6.4",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "snyk": "^1.69.10",
    "style-loader": "^0.18.2",
    "url-loader": "^1.0.1",
    "wait-on": "^2.0.2",
    "webpack": "^4.16.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "assertions-counter": "^0.0.1",
    "aurelia-protractor-plugin": "^1.0.3",
    "aurelia-testing": "^1.0.0-beta.3.0.1",
    "aurelia-tools": "^2.0.0",
    "babel-eslint": "^8.1.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-istanbul": "^4.1.4",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-react-16-adapter-setup": "^0.1.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.7.0",
    "html-hint": "^0.2.4",
    "istanbul": "^0.4.5",
    "istanbul-combine": "^0.3.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.6.4",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "jest-environment-jsdom": "^22.4.3",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-viewport": "^1.0.2",
    "karma-webpack": "^2.0.4",
    "protractor": "^5.1.2",
    "redux-devtools": "^3.4.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^17.0.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "\\.spec\\.(ts|js)x?$",
    "setupFiles": [
      "<rootDir>/test/jest-pretest.js"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!src/components/*.js",
      "!src/classes/*.js",
      "!src/commons/*.js",
      "!**/*.spec.{js,ts}",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "<rootDir>/test/coverage-jest",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "snyk": true
}
