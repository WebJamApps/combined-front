  "scripts": {
    "start": "nps",
    "test": "cross-env NODE_ENV=test nps test.karma",
    "cleaninstall": "rm -rf bower_components && rm -rf yarn.lock && rm -rf node_modules && npm i -g yarn && yarn cache clean && yarn install",
    "postinstall": "yarn global add bower && bower install && npm run vulc && npm start -- build",
    "vulc": "rm -rf static/includes.html && vulcanize --inline-scripts --inline-css includes.html > static/includes.html",
    "prod": "npm run vulc && npm start -- build && npm start -- serve"
  },
